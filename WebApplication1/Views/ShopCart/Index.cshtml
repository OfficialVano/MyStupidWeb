@model ShopCartViewModel
<div class="container">
    @foreach (var el in Model.ShopCart.Items)
    {
    <div class="alert alert-warning mt-3">
        <b>Автомобиль:</b> @el.Product.Name<br/>
        <b>Цена:</b> @el.Product.Price.ToString("c")<br/>
    </div>
    }
    <div class="btn btn-danger" asp-controller="Order">Оплатить</div> 
</div>